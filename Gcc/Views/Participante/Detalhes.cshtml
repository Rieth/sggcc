@model Gcc.Models.ParticipanteGrupo

@{
    ViewBag.Title = "Detalhes";
}

<h2>Detalhes Participantes</h2>
<div class="container">
    <div class="form-group top15 col-lg-6 col-md-6 col-xs-12">
        <div class="form-group">
            <b>@Html.DisplayNameFor(model => model.Cliente.Nome): </b>
            @Html.DisplayFor(model => model.Cliente.Nome, new { @class = "form-control" })
        </div>

        <div class="form-group">
            <b>@Html.DisplayNameFor(model => model.Cliente.Email): </b>
            @Html.DisplayFor(model => model.Cliente.Email, new { @class = "form-control" })
        </div>

    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
